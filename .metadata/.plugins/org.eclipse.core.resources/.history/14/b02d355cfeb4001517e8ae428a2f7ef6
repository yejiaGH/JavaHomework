
public class Goods implements IGoods{
	String name;
	float price;
	int number;
	float total;
	@Override
	public String getName() {
		// TODO Auto-generated method stub
		return name;
	}
	@Override
	public int getNumber() {
		// TODO Auto-generated method stub
		return number;
	}
	@Override
	public float getPrice() {
		// TODO Auto-generated method stub
		return price;
	}
	@Override
	public float getTotal() {
		// TODO Auto-generated method stub
		return number*price;
	}
}
