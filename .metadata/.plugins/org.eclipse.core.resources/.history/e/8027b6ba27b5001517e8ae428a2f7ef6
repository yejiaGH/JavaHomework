package com.yejia.haitao.goods;

public class Goods implements IGoods{
	private String name;
	private float price;

	public Goods(String name, float price){
		this.name = name;
		this.price = price;
	}
	
	@Override
	public String getName() {
		return name;
	}
	
	public float getPrice(){
		return price;
	}
}
